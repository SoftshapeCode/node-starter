        "description": "Registration endpoint",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "description": "",
                "type": "object",
                "x-examples": {
                  "example-1": {
                    "username": "sami",
                    "password": "password",
                    "firstName": "Sami",
                    "lastName": "Adam",
